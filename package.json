{
  "name": "estrangela-cal",
  "version": "1.0.5",
  "description": "Convert from Estrangela ASCII font to CAL code",
  "main": "build/estrangela-cal.js",
  "mainMin": "build/estrangela-cal.min.js",
  "module": "build/estrangela-cal.esm.js",
  "scripts": {
    "pretest": "rollup -c",
    "test": "mocha --reporter test/istanbul.reporter.js --recursive",
    "prebuild": "npm test && npm run lint",
    "build": "npm run bundle",
    "postbuild": "npm run doc",
    "bundle": "rollup -c --environment BUILD:production",
    "build:dev": "rollup -c --environment BUILD:dev",
    "lint": "eslint src test",
    "doc": "jsdoc2md -t doc/readme.hbs src/*.js > README.md",
    "coverage": "cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "prepare": "npm run build"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "no-plusplus": 0,
      "comma-dangle": 0,
      "no-nested-ternary": 0,
      "function-paren-newline": 0,
      "import/extensions": 0,
      "arrow-parens": 0
    }
  },
  "files": [
    "build/*.js"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peshitta/estrangela-cal.git"
  },
  "keywords": [
    "Estrangela",
    "Estrangelo",
    "CAL",
    "Peshitta",
    "Peshitto",
    "Pshitta",
    "Pshitto",
    "Sedra.js",
    "Sedrajs",
    "Sedra",
    "Aramaic",
    "Syriac"
  ],
  "author": "Greg Borota",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peshitta/estrangela-cal/issues"
  },
  "homepage": "https://github.com/peshitta/estrangela-cal#readme",
  "dependencies": {
    "aramaic-mapper": "^1.0.6",
    "cal-code-util": "^1.0.7",
    "estrangela-code-util": "^1.0.7"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.7.0",
    "istanbul": "^0.4.5",
    "jsdoc-to-markdown": "^3.0.0",
    "mocha": "^4.0.1",
    "rollup": "^0.50.0",
    "rollup-plugin-buble": "^0.16.0",
    "rollup-plugin-istanbul": "^1.1.0",
    "rollup-plugin-uglify": "^2.0.1"
  }
}
